    <fieldset class="details">
        <legend>Details</legend>
        <input type="hidden" name="PsgrID" value="{{row.PsgrID}}">
        <table>
            <tr><td>Phone:</td>
                <td><input type="tel"
                name="Phone" value="{{row.Phone}}"></td></tr>
            <tr><td>Street Address:</td>
                <td><input type="text"
                name="StreetAddress" value="{{row.StreetAddress}}"></td></tr>
            <tr><td>StreetLine2:</td>
                <td><input type="text"
                name="StreetLine2" value="{{row.StreetLine2}}"></td></tr>
            <tr><td>City:</td>
                <td><input type="text"
                name="City" value="{{row.City}}"></td></tr>
            <tr><td>State:</td>
                <td><input list="states"
                name="State" value="{{row.State}}"></td></tr>
            <tr><td>Zip:</td>
                <td><input type="text"
                name="Zip" value="{{row.Zip}}"></td></tr>
        </table>
    </fieldset>
</div>
